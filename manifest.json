{
  "display_information": {
    "name": "js.bolt.http"
  },
  "features": {
    "bot_user": {
      "display_name": "js.bolt.http",
      "always_online": true
    }
  },
  "oauth_config": {
    "scopes": {
      "bot": ["chat:write", "channels:history"]
    }
  },
  "settings": {
    "org_deploy_enabled": true,
    "event_subscriptions": {
      "bot_events": ["message.channels"],
      "request_url": "https://example.com"
    }
  }
}
