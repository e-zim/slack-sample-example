{
  "type": "webhook",
  "name": "Status sharing",
  "description": "Update a channel with commit details",
  "workflow": "#/workflows/follow_recent_commits",
  "inputs": {
    "commit_message": {
      "value": "{{data.github_payload_head_commit_message}}"
    },
    "commit_repository": {
      "value": "{{data.github_payload_repository}}"
    }
  }
}
